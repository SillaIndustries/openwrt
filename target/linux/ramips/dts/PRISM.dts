&ethernet {
	mtd-mac-address = <&factory 0x28>;
};

&wmac {
	status = "okay";
	ralink,mtd-eeprom = <&factory 0x0>;
};

&spi0 {
	status = "okay";

	m25p80@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;
		m25p,chunked-io = <32>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
				read-only;
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};

			partition@50000 {
				label = "firmware";
				reg = <0x50000 0x1fb0000>;
				compatible = "denx,uimage";
			};
		};
	};
};

&uart1 {
	status = "okay";
};


&uart2 {
	status = "okay";
};

&i2c {
	status = "okay";
  
	ds1307@68 {
		compatible = "maxim,ds1307";
	       	reg = <0x68>;
	       	status = "okay";
	};
};

//test SD da linkit
&sdhci {
	status = "okay";
	mediatek,cd-low;
};

&ehci {
	status = "okay";
};

&ohci {
	status = "okay";
};
